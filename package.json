{
  "name": "gmail-getter",
  "version": "1.0.0",
  "description": "A simple tool that gets emails from the Gmail API",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "get-refresh-token": "dist/get-token-script.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "generate-schema": "ts-json-schema-generator --path 'src/**/types/*.ts' -o 'tests/schema.json'",
    "test": "jest",
    "format": "prettier . --write --list-different",
    "lint": "prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bormando/gmail-getter.git"
  },
  "keywords": [
    "gmail",
    "test",
    "api"
  ],
  "author": "Dmitrii Bormotov",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/jest-json-schema": "^6.1.1",
    "dotenv": "^16.0.3",
    "jest": "^29.4.3",
    "jest-json-schema": "^6.1.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-json-schema-generator": "^1.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^2.1.1",
    "axios": "^1.3.2"
  }
}
